<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BeefIt.WorkoutEntryPage"
             BackgroundColor="Black">
  <VerticalStackLayout Padding="20" Spacing="20" VerticalOptions="CenterAndExpand">
    <Button x:Name="AllExercisesButton"
            Text="All Exercises"
            BackgroundColor="Gray"
            TextColor="White"
            Clicked="AllExercisesButton_Clicked" />
    <Button x:Name="MyExercisesButton"
            Text="My Exercises"
            BackgroundColor="Gray"
            TextColor="White"
            Clicked="MyExercisesButton_Clicked" />
    <Button x:Name="HistoryButton"
            Text="History"
            BackgroundColor="Gray"
            TextColor="White"
            Clicked="HistoryButton_Clicked" />
  </VerticalStackLayout>
</ContentPage>

